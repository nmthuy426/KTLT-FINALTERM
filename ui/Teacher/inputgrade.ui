           <string notr="true">/* === Bảng chính (QTableWidget) === */
QTableWidget {
    gridline-color: #000000; /* Màu đường kẻ ô */
    font: 2000 12px &quot;Arial&quot;; /* Kích thước chữ */
    background-color: white; /* Nền trắng */
    border: none; /* Xóa viền thừa */
    border-bottom-left-radius: 20px;  /* Bo góc dưới */
    border-bottom-right-radius: 20px;
}

/* === Header (Dòng tiêu đề) === */
QHeaderView::section {
    background-color: navy; /* Màu nền tiêu đề */
    color: white; /* Màu chữ tiêu đề */
    border: 1px solid white; /* Viền trắng để tách cột */
    font: 2000 12px &quot;Arial&quot;;
    padding: 6px; /* Khoảng cách chữ trong header */
}

/* === Thanh cuộn dọc === */
QTableWidget::verticalScrollBar {
    width: 6px;  /* Độ rộng nhỏ hơn */
	padding: 2px;
}

QScrollBar:vertical {
    border: none;
    background: transparent;
    width: 6px;  /* Làm thanh nhỏ */
    margin: 15px 0px 15px 0px; /* Tạo khoảng cách với mép */
}

/* Thanh trượt (cái phần kéo được) */
QScrollBar::handle:vertical {
    background: #909090;  /* Màu trung tính */
    min-height: 30px;  /* Đảm bảo không quá nhỏ */
    border-radius: 3px;  /* Bo góc */
}

/* Khi hover vào thanh kéo */
QScrollBar::handle:vertical:hover {
    background: #707070; /* Đổi màu tối hơn chút */
}

/* Ẩn nút mũi tên trên/dưới */
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: none;
    height: 0px;
}

/* === Thanh cuộn ngang === */
QTableWidget::horizontalScrollBar {
    height: 6px;  /* Mỏng hơn */
}

QScrollBar:horizontal {
    border: none;
    background: transparent;
    height: 6px;  
    margin: 0px 15px 0px 15px;
}

/* Thanh trượt ngang */
QScrollBar::handle:horizontal {
    background: #909090;
    min-width: 30px;
    border-radius: 3px;
	padding: 3px;
}

QScrollBar::handle:horizontal:hover {
    background: #707070;
}

/* Ẩn góc giao giữa 2 thanh kéo */
QScrollBar::corner {
    background: transparent;
    border: none;
}
