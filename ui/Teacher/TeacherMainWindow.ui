                <string notr="true">/* Xóa viền, giữ bo góc */
QTableWidget {
    border: none;  
    border-radius: 15px; 
    background-color: white;
    gridline-color: transparent;
    padding: 5px;
}

/* Bo góc đúng cho bảng */
QTableWidget::viewport {
    border: none;
    border-radius: 15px; /* Đảm bảo toàn bộ bảng bo góc */
    background: white;
}

/* Header trên cùng */
QHeaderView::section {
    border: none;
    background: white;
    padding: 5px;
}

/* Bo góc header bên trái &amp; phải */
QHeaderView::section:first {
    border-top-left-radius: 15px;
    margin-left: -1px; /* Căn chỉnh để không lệch */
}

QHeaderView::section:last {
    border-top-right-radius: 15px;
}

/* Bo góc ô đầu tiên bên trái */
QTableWidget::item:first-child {
    border-top-left-radius: 15px;
    border-bottom-left-radius: 15px;
    background: white;
}

/* Bo góc ô cuối cùng bên phải */
QTableWidget::item:last-child {
    border-top-right-radius: 15px;
    border-bottom-right-radius: 15px;
}

/* Thanh cuộn ngang &amp; dọc */
QScrollBar:horizontal, 
QScrollBar:vertical {
    border: none;
    background: transparent;
    margin: 5px;
}

/* Nút kéo của thanh cuộn */
QScrollBar::handle:horizontal, 
QScrollBar::handle:vertical {
    background: #00008B;
    border-radius: 4px;
}

/* Ẩn mũi tên trên thanh cuộn */
QScrollBar::add-line:horizontal, 
QScrollBar::sub-line:horizontal,
QScrollBar::add-line:vertical, 
QScrollBar::sub-line:vertical {
    width: 0px;
    height: 0px;
}
